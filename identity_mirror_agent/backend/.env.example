# Backend environment variables
# Copy this file to .env and fill in values as needed

# Postgres connection string (update with your local credentials)
DATABASE_URL=postgresql+psycopg://postgres:postgres@localhost:5432/identity_mirror

# Anthropic API key (optional for offline fallback)
ANTHROPIC_API_KEY=

# Frontend origin for CORS
FRONTEND_ORIGIN=http://localhost:3000
